    <ImageView
        android:id="@+id/im_cal_pluse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="176dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/im_cal_minus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="59dp"
        android:layout_marginTop="71dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/im_cal_pluse"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/im_cal_equal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="76dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/im_cal_minus"
        app:srcCompat="@mipmap/ic_launcher_round" />

    <ImageButton
        android:id="@+id/btn_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="145dp"
        android:layout_marginEnd="154dp"
        android:layout_marginBottom="63dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@mipmap/back_button_foreground"/>

    <TextView
        android:id="@+id/tv_cal_equal"
        android:layout_width="172dp"
        android:layout_height="80dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="81dp"
        android:text="왼쪽 값과 오른쪽 값이 같다는 의미"
        android:fontFamily="@font/font"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/im_cal_equal"
        app:layout_constraintTop_toBottomOf="@+id/tv_cal_minus" />

    <TextView
        android:id="@+id/tv_cal_pluse"
        android:layout_width="153dp"
        android:layout_height="75dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="176dp"
        android:text="왼쪽 값과 오른쪽 값을 더한다는 의미"
        android:fontFamily="@font/font"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/im_cal_pluse"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_cal_minus"
        android:layout_width="160dp"
        android:layout_height="76dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="60dp"
        android:text="왼쪽 값에서 오른쪽 값을 뺀다는 의미"
        android:fontFamily="@font/font"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/im_cal_minus"
        app:layout_constraintTop_toBottomOf="@+id/tv_cal_pluse" />
</androidx.constraintlayout.widget.ConstraintLayout>